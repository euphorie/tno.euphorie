<registry xmlns:i18n="http://xml.zope.org/namespaces/i18n"
          i18n:domain="euphorie"
>
  <record name="euphorie.client_url">
    <value purge="false">https://instrumenten.rie.nl</value>
  </record>
  <record name="euphorie.allow_guest_accounts">
    <value purge="false">True</value>
  </record>
  <record name="euphorie.allow_social_sharing">
    <value purge="false">True</value>
  </record>
  <record name="euphorie.allow_user_defined_risks">
    <value purge="false">True</value>
  </record>
  <record name="euphorie.default_country">
    <value purge="false">nl</value>
  </record>
  <record name="euphorie.library">
    <value purge="false"></value>
  </record>
  <record name="euphorie.max_login_attempts">
    <value purge="false">3</value>
  </record>
  <record name="euphorie.use_clone_feature">
    <value purge="false">True</value>
  </record>
  <record name="plonetheme.nuplone.appconfigtile_actions">
    <value purge="false">{
  "tiles": "\neuphorie.search\neuphorie.versions [view_type=='view' and context.portal_type in ['euphorie.module', 'euphorie.risk', 'euphorie.solution', 'euphorie.survey', 'euphorie.surveygroup'] ]\nlanguage",
  "type": "group",
  "id": "portlets",
  "wrapper": "div"
}</value>
  </record>
</registry>
