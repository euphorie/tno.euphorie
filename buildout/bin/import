#!/bin/sh

set -e

function import() {
	if [ -f data/$4.xml ] ; then
		login="$(echo $2 | tr A-Z a-z)"
		echo Importing $4 - $1
		bin/instance run src/Euphorie/importrie.py \
			--country=nl \
			--login=$login \
			--sector="$3" \
			--name="$1" \
			--version-name="Standaard" \
			data/$4.xml
	else
		echo "Can not import $4.xml - file does not exist."
	fi
}

